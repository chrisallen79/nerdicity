{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      { "devDependencies": true }
    ],
    "no-console": "off",
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [
      1,
      { "extensions": [".js", ".jsx"] }
    ],
    "react/jsx-fragments": [1, "element"]
  }
}
